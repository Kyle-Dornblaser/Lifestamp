<p id="notice"><%= notice %></p>

<h1>Listing Lifestamps</h1>

<table>
  <thead>
    <tr>
      <th>Value</th>
      <th>Timestamp</th>
      <th>Topic</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @lifestamps.each do |lifestamp| %>
      <tr>
        <td><%= lifestamp.value %></td>
        <td><%= lifestamp.timestamp %></td>
        <td><%= lifestamp.topic %></td>
        <td><%= link_to 'Show', lifestamp %></td>
        <td><%= link_to 'Edit', edit_lifestamp_path(lifestamp) %></td>
        <td><%= link_to 'Destroy', lifestamp, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Lifestamp', new_lifestamp_path %>
